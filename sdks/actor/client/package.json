{
	"name": "@rivet-gg/actor-client",
	"version": "24.6.2-rc.1",
	"files": ["src", "dist", "package.json"],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/mod.d.ts",
				"default": "./dist/mod.js"
			},
			"require": {
				"types": "./dist/mod.d.cts",
				"default": "./dist/mod.cjs"
			}
		},
		"./test": {
			"import": {
				"types": "./dist/test.d.ts",
				"default": "./dist/test.js"
			},
			"require": {
				"types": "./dist/test.d.cts",
				"default": "./dist/test.cjs"
			}
		},
		"./unstable-react": {
			"import": {
				"types": "./dist/unstable-react.d.ts",
				"default": "./dist/unstable-react.js"
			},
			"require": {
				"types": "./dist/unstable-react.d.cts",
				"default": "./dist/unstable-react.cjs"
			}
		}
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsup src/mod.ts src/test.ts src/unstable-react.tsx",
		"check-types": "tsc --noEmit"
	},
	"dependencies": {
		"@rivet-gg/actor-common": "^24.6.2-rc.1",
		"@rivet-gg/actor-protocol": "^24.6.2-rc.1",
		"@rivet-gg/manager-protocol": "^24.6.2-rc.1",
		"@std/assert": "npm:@jsr/std__assert@^1.0.10",
		"@std/cbor": "npm:@jsr/std__cbor@^0.1.4",
		"zod": "^3.24.1"
	},
	"optionalDependencies": {
		"react": "^19.0.0"
	},
	"devDependencies": {
		"@kentcdodds/tmp-react-server-dom-esm": "^19.0.1",
		"@types/react": "^19.0.4",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3"
	}
}
